<h1>Register account</h1>
<%= form_for @changeset, registration_path(@conn, :create), fn f -> %>
  <div>
    <%= label f, :username %> 
  </div>
  <div>
    <%= text_input f, :username %>
    <%= error_tag f, :username %>
  </div>
  <div>
    <%= label f, :password %>
  </div>
  <div>
    <%= password_input f, :password %>
    <%= error_tag f, :password %>
  </div>
  <%= submit "Submit" %>
<% end %> 
